<?xml version="1.0"?>
<object name="portal_javascripts">

    <!-- BBB: Create bundles for jQuery, jQueryTools and TinyMCE - see
         registry.xml -->
    <javascript bundle="jquery"
        id="jquery.js" />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.js"  />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.form.js"  />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.overlayhelpers.js"  />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.plugins.js"  />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.dateinput.js"  />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.rangeinput.js"  />
    <javascript bundle="jquerytools"
        id="++resource++plone.app.jquerytools.validator.js"  />
    <javascript bundle="tinymce"
        id="tiny_mce.js" />
    <javascript bundle="tinymce"
        id="tiny_mce_init.js" />

    <!-- Toolabar -->
    <javascript 
        cacheable="False" compression="safe" cookable="False"
        enabled="True" inline="False" bundle="default"
        id="++resource++plone.app.toolbar/toolbar/src/toolbar.js"
        />
    <!-- TODO: toolbar_overlays.js -->
    <!-- TODO: notify -->

    <!-- Twitter Bootstrap -->
    <javascript 
        cacheable="False" compression="none" cookable="False"
        enabled="True" inline="False" bundle="bootstrap"
        id="++resource++plone.app.toolbar/bootstrap/js/bootstrap.min.js"
        />

</object>
